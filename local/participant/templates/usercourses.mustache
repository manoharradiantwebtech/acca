 <div class="main-wrapper">
        <a class="btn btn-warning btn-sm rounded-pill px-3 support-button" href=""><i class="fa fa-envelope mr-1"></i> Support</a>
        <!-- HEADER -->
    
        <!-- MAIN CONTENT -->
        <main class="main-content">
            <!-- PAGE HEADING -->
            <div class="page-header pt-0 pb-0">
                <div class="container-fluid">
                    <div class="row justify-content-between align-items-end">
                        <div class="col-md-auto page-header-left pt-2">
                            <h4>My Courses</h4>
                            <p class="mb-0">View all your courses at one place</p>
                        </div>
                        <div class="col-md-auto page-header-right pt-2">
                            <span class="fw600 text-dark pr-3">Filter</span>
                            <div class="btn-group btn-group-toggle filter-btn-group" data-toggle="buttons">
                                <label class="btn btn-light btn-sm allcourses px-4 active">
                                    <input type="radio" name="options" id="option1" checked> All
                                </label>
                                <label class="btn btn-light btn-sm in-progress">
                                    <input type="radio" name="options" id="option2"> In Progress
                                </label>
                                <label class="btn btn-light btn-sm completed completed-course">
                                    <input type="radio" name="options" id="option3"> Completed
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- TIMELINE -->
            <div class="section-main-content">
                <div class="container-fluid">
                        <div class="courses-list course-list-page bg-white">
                            {{#enrolled_courses}}
                             <div class="course-list-block enrolled_course">
                                 <div class="row justify-content-between">
                                     <div class="col-lg-7 col-6">
                                         <div class="row no-gutters">
                                             <div class="col-lg-12 pl-3 pt-3 col-auto">
                                                <h6 class="mb-2 pt-1"><a href={{course_url}}>{{coursename}}</a></h6>
                                                <p class="text-prime-light-1 small mb-0">{{count}} Lessons {{#expireday}}<span class="bg-warning ml-2 py-1 px-2 small fw600 text-dark" style="height: 20px;">Expires on {{expireday}}</span>{{/expireday}}</p></p>
                                             </div>
                                         </div>
                                     </div>
                                     <div class="col-lg-auto col-6">
                                         <div class="d-flex flex-row align-items-center h-100">
                                              {{#isresume}}
                                               <span> <a class="btn resume-btn" href={{viewurl}}>Resume</a></span>
                                             {{/isresume}}
                                              <div class="d-inline-block pl-2">
                                                 <div class="course-progress p-3">
                                            <div class="posi-bottom">
                                                                          <div class="progress-block">
                                                                         <svg width="0" height="0" style="opacity: 0; position: absolute;">
                                                                             <defs>
                                                                             <linearGradient id="linear" x1="0%" y1="0%" x2="100%" y2="0%">
                                                                                 <stop offset="0%" stop-color="#00f796"></stop>
                                                                                               <stop offset="100%" stop-color="#00b7f4"></stop>
                                                                                                         </linearGradient>
                                                                              </defs>
                                                                                 <rect x="10" y="10" width="600" height="200" fill="url(#linear)"></rect>
                                                                                  </svg>
                                                                                  <section class="svg-container">
                                                                                    <svg class="radial-progress" data-countervalue="{{{course_completion.completion_percentage}}}" viewBox="0 0 80 80">
                                                                                                                                                            <circle class="bar-static-outer" cx="40" cy="40" r="40"></circle>
                                                                                                                                                            <circle class="bar-static" cx="40" cy="40" r="34"></circle>
                                                                                                                                                            <circle class="bar-static-inner" cx="40" cy="40" r="31"></circle>
                                                                                                                                                           <circle class="bar--animated" cx="40" cy="40" r="35" style="stroke-dashoffset: {{{course_completion.dashoffset}}};"></circle>
                                                                                                                                                           <text class="countervalue start" x="50%" y="57%" transform="matrix(0, 1, -1, 0, 80, 0)">{{{course_completion.completion_percentage}}}</text>
                                                                                        </svg>
                                                                                    </section>
                                                                                   </div>
                                                                                 </div>

 <!--<p class="small fw500 mb-0 text-center">Course<span class="d-block"></span>Completed</p>-->
                                        </div>
                                            </div>
                                         </div>
                                     </div>
                                 </div>
                             </div>
                          {{/enrolled_courses}}
                            

                        
{{#completed_courses}}
                             <div class="course-list-block completedc">
                                 <div class="row justify-content-between">
                                     <div class="col-lg-7 col-12">
                                         <div class="row no-gutters">
                                              
                                             <div class="col-lg-12 pl-3 pt-3 col-auto">
                                                <h6 class="mb-2 pt-1">{{coursename}}</h6>
                                                <p class="text-prime-light-1 small mb-0">{{count}} Lessons</p>
                                             </div>
                                         </div>
                                     </div>
                                     <div class="col-lg-auto col-12">
                                         <div class="d-flex flex-row align-items-center h-100">
                                            <div class="inline-block text-right mr-3">
                                                    <p class="text-muted small mb-0">Completed on</p>
                                                    <div class="small fw500">{{completiondate}}</div>
                                                 
                                                </div>
                                              {{#view_certificate}}
                                             <div class="buttons-group my-2">
                                                <a class="btn btn-outline-prime btn-sm my-1" href={{view_certificate}}>View Certificate</a>
                                            </div>
                                            {{/view_certificate}}
                                            <div class="d-inline-block pl-2">
                                                 <div class="course-progress p-3">
                                           <div class="posi-bottom">
                                                                                                                     <div class="progress-block">
                                                                                                                    <svg width="0" height="0" style="opacity: 0; position: absolute;">
                                                                                                                        <defs>
                                                                                                                        <linearGradient id="linear" x1="0%" y1="0%" x2="100%" y2="0%">
                                                                                                                            <stop offset="0%" stop-color="#00f796"></stop>
                                                                                                                                          <stop offset="100%" stop-color="#00b7f4"></stop>
                                                                                                                                                    </linearGradient>
                                                                                                                         </defs>
                                                                                                                            <rect x="10" y="10" width="600" height="200" fill="url(#linear)"></rect>
                                                                                                                             </svg>
                                                                                                                             <section class="svg-container">
                                                                                                                               <svg class="radial-progress" data-countervalue="{{{course_completion.completion_percentage}}}" viewBox="0 0 80 80">
                                                                                                                                                                                                       <circle class="bar-static-outer" cx="40" cy="40" r="40"></circle>
                                                                                                                                                                                                       <circle class="bar-static" cx="40" cy="40" r="34"></circle>
                                                                                                                                                                                                       <circle class="bar-static-inner" cx="40" cy="40" r="31"></circle>
                                                                                                                                                                                                      <circle class="bar--animated" cx="40" cy="40" r="35" style="stroke-dashoffset: {{{course_completion.dashoffset}}};"></circle>
                                                                                                                                                                                                      <text class="countervalue start" x="50%" y="57%" transform="matrix(0, 1, -1, 0, 80, 0)">{{{course_completion.completion_percentage}}}</text>
                                                                                                                                   </svg>
                                                                                                                               </section>
                                                                                                                              </div>
                                                                                                                            </div>

                                            <!-- <p class="small fw500 mb-0 text-center">Course<span class="d-block"></span>Completed</p>-->
                                        </div>
                                            </div>
                                         </div>
                                     </div>
                                 </div>
                             </div>
                             {{/completed_courses}}
                             
                        </div>
                </div>
            </div>
        </main>